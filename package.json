{
  "name": "gujarat-landchain-juliaos",
  "version": "1.0.0",
  "description": "AI-Powered Land Registry System Built with JuliaOS Framework",
  "main": "src/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "anchor:build": "anchor build",
    "anchor:deploy": "anchor deploy",
    "anchor:test": "anchor test",
    "solana:devnet": "solana config set --url devnet",
    "solana:mainnet": "solana config set --url mainnet-beta",
    "julia:test": "julia --project=. test/runtests.jl",
    "julia:agents": "julia --project=. scripts/init_agents.jl"
  },
  "keywords": [
    "blockchain",
    "solana",
    "juliaos",
    "ai-agents",
    "swarms",
    "land-registry",
    "government",
    "dapp"
  ],
  "author": "Gujarat LandChain Development Team",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.95.2",
    "@solana/spl-token": "^0.4.8",
    "@coral-xyz/anchor": "^0.30.1",
    "next": "^15.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.10",
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1",
    "axios": "^1.7.7",
    "ws": "^8.18.0",
    "express": "^4.19.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.0.0",
    "typescript": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
